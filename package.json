{
  "name": "react-redux-router-starterkit",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@storybook/react": "^3.3.10",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "history": "^4.7.2",
    "mirror-creator": "^1.1.0",
    "prop-types": "^15.6.0",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-redux": "^5.0.6",
    "react-responsive": "^4.0.3",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "react-scripts": "1.0.17",
    "react-test-renderer": "^16.3.1",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-actions": "^2.3.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json}": [
      "prettier --parser --write",
      "prettier --single-quote --tab-width=2 --print-width=120 --write",
      "git add"
    ],
    "src/**/*.{scss, css}": [
      "stylelint",
      "prettier --parser postcss --tab-width=2 --print-width=120 --write",
      "git add"
    ]
  },
  "scripts": {
    "precommit": "lint-staged",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "flow":  "node node_modules/.bin/flow",
    "start-js": "react-scripts start",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "node node_modules/.bin/jest",
    "eject": "react-scripts eject"
  },
  "jest": {
    "testEnvironment": "enzyme",
    "testEnvironmentOptions": {
      "enzymeAdapter": "react16"
    },
    "moduleNameMapper": {
      "\\.(css)$": "<rootDir>/node_modules/jest-css-modules"
    }
  },
  "devDependencies": {
    "axios": "^0.17.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "7",
    "babel-jest": "^22.4.1",
    "babel-plugin-css-modules-transform": "^1.5.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react-app": "^3.1.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "3.x",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.7.0",
    "flow-bin": "^0.61.0",
    "flow-watch": "^1.1.3",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "jest-css-modules": "^1.1.0",
    "jest-environment-enzyme": "^6.0.0",
    "json-server": "^0.12.1",
    "lint-staged": "^7.0.4",
    "node-sass-chokidar": "^1.2.2",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.10.2",
    "redux-devtools-extension": "^2.13.2",
    "storybook": "^1.0.0"
  }
}
